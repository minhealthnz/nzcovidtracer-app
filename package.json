    "react-native-share": "^7.2.0",
    "react-native-splash-screen": "^3.2.0",
    "react-redux": "^7.2.1",
    "reactotron-redux-saga": "^4.2.3",
    "realm": "^6.1.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.1",
    "url": "^0.11.0",
